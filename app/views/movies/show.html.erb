<header id="home" class="jumbotron">

  <div class="container">
  
    <div class="row">
    
      <div class="col-sm-6 text-col">
      
        <h1><%= @movie.title %></h1><br>
        <h3>Created by: <%= @movie.user.name %></h3>
        
      </div>
      
      <div class="col-sm-6">
        <div class="imac-frame">
          <%= image_tag("tv.png", class: "img-responsive img-center", height: 500, width: 600, style: "position: relative;") %>
          <!-- <img class="img-responsive img-center" src="../assets/imac.png" alt=""/> -->
          <div class="imac-screen flexslider">
            <div class="reel" style="position: relative; left: 0px; top: 140px;">
              <%= render 'reel' %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  
 </header>
 <!-- End header  -->



